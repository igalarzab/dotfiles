#!/bin/bash
# List all the JPG and ARW files and deletes all the ARW without a JPG correspondence
#
# Author: @igalarzab
#

FILES_TO_DELETE=$(comm -23 <(ls *ARW | sort | sed 's/\(.*\)..../\1/') <(ls *JPG | sort | sed 's/\(.*\)..../\1/'))

for file in $FILES_TO_DELETE; do
    echo $f.ARW
    trash $f.ARW
done
