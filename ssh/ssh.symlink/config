Host *
    AddKeysToAgent yes
    ServerAliveInterval 60
    ServerAliveCountMax 5

Host github.com
    AddKeysToAgent yes

Host *.amazonaws.com
    User ubuntu
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    LogLevel FATAL

Host vagrant
    HostName 127.0.0.1
    User vagrant
    Port 2222
    IdentityFile ~/.vagrant.d/insecure_private_key
    IdentitiesOnly yes
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    LogLevel FATAL
